<section id="faq">
    <div class="container text-center">
        <h2 class="mb-5"><%- title %></h2>
        <div class="text-left accordion" id="accordion_faq">
            <% qas.forEach(function(qa,i) { %>
                <div class="card">
                    <div class="card-header">
                        <button class="btn btn-link d-table-cell" type="button" 
                                data-toggle="collapse" data-target="#collapse<%- i %>" 
                                aria-expanded="false" aria-controls="collapse<%- i %>">
                                <div class="justify-content-between align-items-center">
                                    <h5 class="mb-0"><%- qa.question %></h5>
                                    <div class="icon">
                                        <i class="fal fa-arrow-circle-down"></i>
                                        <i class="fal fa-arrow-circle-up"></i>
                                    </div>
                                </div>
                        </button>
                    </div>
                    <div id="collapse<%- i %>" class="collapse" 
                            aria-labelledby="heading<%- i %>" data-parent="#accordion_faq">
                        <div class="card-body d-table">
                            <p class="mb-0">A: <%- qa.answer %></p>
                        </div>
                    </div>
                </div>
            <% }) %>
        </div>
    </div>
</section>