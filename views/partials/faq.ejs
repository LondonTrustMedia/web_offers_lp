<section id="faq">
    <div class="container text-center">
        <h2 class="mb-5"><%- title %></h2>
        <div class="text-left accordion" id="accordion_faq">
            <% qas.forEach(function(qa,i) { %>
                <div class="card">
                    <div class="card-header">
                        <h2 class="mb-0 d-table">
                            <div class="d-table-cell pr-md-5 pr-3 Q">Q</div>
                            <button class="btn btn-link d-table-cell py-p px-0" type="button" 
                                    data-toggle="collapse" data-target="#collapse<%- i %>" 
                                    aria-expanded="false" aria-controls="collapse<%- i %>">
                                <%- qa.question %>
                            </button>
                        </h2>
                    </div>
                    <div id="collapse<%- i %>" class="collapse" 
                        aria-labelledby="heading<%- i %>" data-parent="#accordion_faq">
                        <div class="card-body d-table">
                            <div class="d-table-cell pr-md-5 pr-3 A">A</div>
                            <p class="d-table-cell"><%- qa.answer %></p>
                        </div>
                    </div>
                </div>
            <% }) %>
        </div>
    </div>
</section>